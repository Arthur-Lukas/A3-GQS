<?php
class LivroFisico {
    public $id;
    public $titulo;
    public $autor;
    public $lancamento;
    public $preco;
    public $genero_id;

    public function __construct($titulo, $autor, $lancamento, $preco, $genero_id) {
        $this->titulo = $titulo;
        $this->autor = $autor;
        $this->lancamento = $lancamento;
        $this->preco = $preco;
        $this->genero_id = $genero_id;
    }
}
?>