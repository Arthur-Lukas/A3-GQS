<?php
class Ebook {
    public $id;
    public $titulo;
    public $autor;
    public $lancamento;
    public $paginas;
    public $genero_id;

    public function __construct($titulo, $autor, $lancamento, $paginas, $genero_id) {
        $this->titulo = $titulo;
        $this->autor = $autor;
        $this->lancamento = $lancamento;
        $this->paginas = $paginas;
        $this->genero_id = $genero_id;
    }
}
?>